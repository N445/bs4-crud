{
  "name": "n445/bs4crud",
  "description": "bs4crud",
  "type": "symfony-bundle",
  "license": "MIT",
  "keywords": [
    "Bootstrap 4 symfony CRUD"
  ],
  "homepage": "https://github.com/devozanges/ozbs4crud",
  "authors": [
    {
      "name": "Ozanges",
      "email": "contact@ozanges.fr",
      "role": "Developer"
    }
  ],
  "require": {
    "symfony/flex": "^1.3.1",
    "symfony/orm-pack": "^1.0",
    "symfony/maker-bundle": "^1.29",
    "symfony/twig-bundle": "5.2.*",
    "symfony/validator": "5.2.*",
    "sensio/framework-extra-bundle": "^6.1",
    "symfony/security-csrf": "5.2.*",
    "symfony/form": "5.2.*"
  },
  "autoload": {
    "psr-4": {
      "Oz\\Bs4Crud\\": "src"
    }
  },
  "minimum-stability": "dev",
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    }
  }
}
